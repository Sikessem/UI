{
	"name": "@sikessem/ui",
	"version": "0.0.0",
	"license": "MIT",
	"packageManager": "pnpm@10.12.4",
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=22.17",
		"pnpm": ">=10.12",
		"npm": "prefer using pnpm",
		"yarn": "prefer using pnpm"
	},
	"type": "module",
	"module": "./src/index.ts",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"files": [
		"src/**/*"
	],
	"exports": {
		".": "./src/index.ts",
		"./package.json": "./package.json"
	},
	"bugs": "https://github.com/sikessem/ui/issues",
	"homepage": "https://sikessem.github.io/ui",
	"author": {
		"name": "Sigui Kess√© Emmanuel",
		"email": "siguici@proton.me",
		"url": "https://github.com/siguici"
	},
	"description": "Design responsive user interfaces with light/dark mode and theme management quickly with this TailwindCSS-based plugin.",
	"keywords": [
		"color-scheme",
		"componants",
		"css",
		"dark-mode",
		"light-mode",
		"plugin",
		"sikessem",
		"tailwindcss",
		"tailwind-color-scheme",
		"theme",
		"typescript",
		"ui",
		"utilities"
	],
	"sideEffects": false,
	"private": false,
	"scripts": {
		"build": "rimraf jsr/* && denoify --out jsr"
	},
	"devDependencies": {
		"@types/node": "^24.0.10",
		"denoify": "^1.6.17",
		"rimraf": "^6.0.1",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.8.3"
	},
	"peerDependencies": {
		"tailwindcss": "^4.1.11"
	},
	"dependencies": {
		"colorwind": "^0.3.0"
	}
}
