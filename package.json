{
    "name": "@sikessem/ui",
    "private": true,
    "type": "module",
    "engines": {
        "node": ">=18.16",
        "pnpm": ">=8.0.0",
        "npm": "prefer using pnpm",
        "yarn": "prefer using pnpm"
    },
    "packageManager": "pnpm@8.6.11",
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://sikessem.github.io/packages/ui",
    "author": {
        "name": "SIGUI Kess√© Emmanuel",
        "email": "contact@sigui.ci",
        "url": "https://sigui.ci"
    },
    "scripts": {
        "build": "tsc && vite build",
        "check": "rome ci .",
        "check.format": "rome format .",
        "check.lint": "rome check .",
        "debug": "pnpm check && pnpm test",
        "dev": "concurrently \"vite --host\" \"vitest\"",
        "fix": "pnpm lint && pnpm format",
        "format": "rome format --write .",
        "inspect": "node --inspect-brk ./node_modules/vite/bin/vite.js --force",
        "lint": "rome check --apply-unsafe .",
        "preview": "vite preview",
        "test": "vitest run",
        "test.ui": "vitest --ui",
        "test.cov": "vitest run --coverage",
        "start": "vite --open"
    },
    "devDependencies": {
        "@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
        "@types/alpinejs": "^3.7.2",
        "@types/node": "^20.4.10",
        "@vitest/coverage-istanbul": "^0.34.1",
        "alpinejs": "^3.12.3",
        "autoprefixer": "^10.4.14",
        "concurrently": "^8.2.0",
        "cssnano": "^6.0.1",
        "postcss": "^8.4.27",
        "postcss-import": "^15.1.0",
        "postcss-load-config": "^4.0.1",
        "postcss-nested": "^6.0.1",
        "rome": "^12.1.3",
        "tailwindcss": "^3.3.3",
        "ts-node": "^10.9.1",
        "tslib": "^2.6.1",
        "typescript": "^5.1.6",
        "vite": "^4.4.9",
        "vite-tsconfig-paths": "^4.2.0",
        "vitest": "^0.34.1"
    }
}
